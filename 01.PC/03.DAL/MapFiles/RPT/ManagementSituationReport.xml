  </alias>
  <statements>

    <!--经营情况统计——汇总-->
    <procedure id="RPT_ManagementSituationReport_SQL01" parameterMap="RPT_ManagementSituationReport_SQL01_ParameteMap" resultClass="ManagementSituationTotalReportUIModel">
